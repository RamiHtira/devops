pipeline {
environment {
		DOCKERHUB_CREDENTIALS=credentials('dockerhub')
	}
    agent any
    
    stages{
        
        stage(' GIT'){
         steps {
            echo 'Pulling...';
            git branch: 'fournisseurs',credentialsId: 'gittoken', url: 'https://github.com/RamiHtira/devops.git'
               }
        }
 	
        stage('Clean'){
            steps {
    
            sh "mvn clean"
               }
            
        }
       
    }
}
